<%= render "form", list: @list %>
<div class="container mt-3">
<h2>New list</h2>
<%= simple_form_for(@list) do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>
  <div class="form-inputs">
    <%= f.input :photo, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary mt-3"%>
  </div>
<% end %>
</div>
